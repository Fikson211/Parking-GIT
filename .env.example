# Port (Railway сам задаёт PORT)
PORT=8080

# Секрет для сессий (обязательно задай в Railway Variables)
SESSION_SECRET=change_me_please

# Railway Postgres (добавь плагин Postgres и скопируй DATABASE_URL в Variables)
DATABASE_URL=postgresql://user:pass@host:port/dbname

# Админ по умолчанию (создастся при первом запуске, если такого телефона ещё нет)
ADMIN_PHONE=79991112233
ADMIN_PIN=1234
ADMIN_FIO=Администратор
